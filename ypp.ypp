%{
    #include "hpp.hpp"
%}

%defines %union { int i; float f; string *s; }

%token <s> SYM
%token <i> INT
%token <f> NUM

%%

REPL :
REPL : REPL ex
ex : NUM        { cout << "num:" <<  $1 << endl; }
ex : INT        { cout << "int:" <<  $1 << endl; }
ex : SYM        { cout << "sym:" << *$1 << endl; }
